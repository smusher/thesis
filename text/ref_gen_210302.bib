@article{alvarez_counting_2002,
	title = {Counting channels: a tutorial guide on ion channel fluctuation analysis},
	volume = {26},
	issn = {1043-4046},
	shorttitle = {Counting channels},
	url = {https://journals.physiology.org/doi/full/10.1152/advan.00006.2002},
	doi = {10.1152/advan.00006.2002},
	abstract = {Ion channels open and close in a stochastic fashion, following the laws of probability. However, distinct from tossing a coin or a die, the probability of finding the channel closed or open is not a fixed number but can be modified (i.e., we can cheat) by some external stimulus, such as the voltage. Single-channel records can be obtained using the appropriate electrophysiological technique (e.g., patch clamp), and from these records the open probability and the channel conductance can be calculated. Gathering these parameters from a membrane containing many channels is not straightforward, as the macroscopic current I = iNPo, where i is the single-channel current, N the number of channels, and Po the probability of finding the channel open, cannot be split into its individual components. In this tutorial, using the probabilistic nature of ion channels, we discuss in detail how i, N, and Po max (the maximum open probability) can be obtained using fluctuation (nonstationary noise) analysis (Sigworth FJ. G Gen Physiol 307: 97–129, 1980). We also analyze the sources of possible artifacts in the determination of i and N, such as channel rundown, inadequate filtering, and limited resolution of digital data acquisition by use of a simulation computer program (available at  www.cecs.cl).},
	number = {4},
	urldate = {2021-02-26},
	journal = {Advances in Physiology Education},
	author = {Alvarez, Osvaldo and Gonzalez, Carlos and Latorre, Ramon},
	month = dec,
	year = {2002},
	note = {Publisher: American Physiological Society},
	pages = {327--341},
	file = {Full Text PDF:/home/sam/Zotero/storage/VC5WETD5/Alvarez et al. - 2002 - Counting channels a tutorial guide on ion channel.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/V7KVN86H/advan.00006.html:text/html},
}

@incollection{heinemann_7_1992,
	series = {Ion {Channels}},
