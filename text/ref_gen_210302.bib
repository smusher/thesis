	file = {Full Text PDF:/home/sam/Zotero/storage/IKKUQIPD/Li et al. - 2005 - Ligand-dependent Linkage of the ATP Site to Inhibi.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/V25ZIHDY/42595.html:text/html},
}

@article{yang_palmitoylation_2020,
