	author = {Speca, David J. and Chihara, Daisuke and Ashique, Amir M. and Bowers, M. Scott and Pierce-Shimomura, Jonathan T. and Lee, Jungsoo and Rabbee, Nusrat and Speed, Terence P. and Gularte, Rodrigo J. and Chitwood, James and Medrano, Juan F. and Liao, Mark and Sonner, James M. and Ii, Edmond I. Eger and Peterson, Andrew S. and {McIntire}, Steven L.},
	urldate = {2021-06-12},
	date = {2010-08-12},
	langid = {english},
	note = {Publisher: Public Library of Science},
	keywords = {Biological locomotion, Caenorhabditis elegans, Anesthetics, Animal behavior, Ethanol, Heterozygosity, Mutagenesis, Point mutation},
	file = {Full Text PDF:/home/sam/Zotero/storage/A5XJXRIY/Speca et al. - 2010 - Conserved Role of unc-79 in Ethanol Responses in L.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/28FIM9UT/article.html:text/html},
}

