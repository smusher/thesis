	date = {2013-08-09},
	langid = {english},
	keywords = {Electrophysiology, Inward Rectifier Channel, Ion Channels, {KATP} Channel, Membrane Biophysics, Potassium Channels, Protein Engineering},
	file = {ScienceDirect Full Text PDF:/home/sam/Zotero/storage/Y8JR7FYQ/Li et al. - 2013 - Decomposition of Slide Helix Contributions to ATP-.pdf:application/pdf;ScienceDirect Snapshot:/home/sam/Zotero/storage/A8P273CR/S002192582045330X.html:text/html},
}

@article{eaton_application_1991,
