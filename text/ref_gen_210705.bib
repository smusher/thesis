@article{tusnady_membrane_1997,
	title = {Membrane topology distinguishes a subfamily of the {ATP}-binding cassette ({ABC}) transporters},
	volume = {402},
	rights = {{FEBS} Letters 402 (1997) 1873-3468 © 2015 Federation of European Biochemical Societies},
	issn = {1873-3468},
	url = {https://febs.onlinelibrary.wiley.com/doi/abs/10.1016/S0014-5793%2896%2901478-0},
	doi = {https://doi.org/10.1016/S0014-5793(96)01478-0},
	abstract = {A group of {ATP}-binding cassette ({ABC}) transporters, including the yeast cadmium transporter ({YCF}1), the mammalian multidrug resistance-associated protein ({MRP}), the multispecific organic anion transporter and its congener ({MOAT} and {EBCR}), as well as the sulfonylurea receptor ({SUR}), group into a subfamily by sequence comparison. We suggest that these {MRP}-related proteins are also characterized by a special, common membrane topology pattern. The most studied {ABC} transporters, the cystic fibrosis transmembrane conductance regulator ({CFTR}) and the multidrug resistance ({MDR}) proteins, were shown to contain a tandem repeat of six transmembrane helices, each set followed by an {ATP}-binding domain. According to the present study, in contrast to various membrane topology predictions proposed for the different {MRP}-related proteins, they all seem to have a {CFTR}/{MDR}-like core structure, and an additional, large, N-terminal hydrophobic region. This latter domain is predicted to contain 4–6 (most probably 5) transmembrane helices, and is occasionally glycosylated on the cell surface. Since all the {MRP}-related transporters were shown to interact with anionic compounds, the N-terminal membrane-bound domain may have a key role in these interactions.},
	pages = {1--3},
	number = {1},
	journaltitle = {{FEBS} Letters},
	author = {Tusnády, Gábor E. and Bakos, Éva and Váradi, András and Sarkadi, Balázs},
	urldate = {2021-01-20},
	date = {1997},
	langid = {english},
	note = {\_eprint: https://febs.onlinelibrary.wiley.com/doi/pdf/10.1016/S0014-5793\%2896\%2901478-0},
	keywords = {{ABC} transporter, {cMOAT}, Membrane topology, {MRP}, {SUR}, {YCF}1},
	file = {Full Text PDF:/home/sam/Zotero/storage/Q6ES6UKX/Tusnády et al. - 1997 - Membrane topology distinguishes a subfamily of the.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/LRTKYQZT/S0014-5793(96)01478-0.html:text/html},
