	journaltitle = {British Journal of Pharmacology},
	author = {Hahn, Suyun and Kim, So Woon and Um, Ki Bum and Kim, Hyun Jin and Park, Myoung Kyu},
	urldate = {2021-06-12},
	date = {2020},
