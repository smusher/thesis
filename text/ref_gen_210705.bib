	shortjournal = {Psychonomic Bulletin \& Review},
	author = {Wagenmakers, Eric-Jan},
	urldate = {2021-01-20},
	date = {2007-10-01},
	langid = {english},
	file = {Springer Full Text PDF:/home/sam/Zotero/storage/Z72DYGCR/Wagenmakers - 2007 - A practical solution to the pervasive problems ofp.pdf:application/pdf},
}

