	pages = {599--608},
	file = {Full Text PDF:/home/sam/Zotero/storage/6X8JRLTD/Shyng et al. - 2000 - Structural Determinants of Pip2 Regulation of Inwa.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/BLVERQK6/Structural-Determinants-of-Pip2-Regulation-of.html:text/html},
}

@article{macgregor_nucleotides_2002,
	title = {Nucleotides and phospholipids compete for binding to the {C} terminus of {KATP} channels},
