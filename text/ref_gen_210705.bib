	file = {Full Text PDF:/home/sam/Zotero/storage/GKDXT6ZH/Cridge et al. - 2018 - Eukaryotic translational termination efficiency is.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/6D8NVUK2/4793374.html:text/html},
}

@article{sun_relationships_2005,
	title = {Relationships Among Stop Codon Usage Bias, Its Context, Isochores, and Gene Expression Level in Various Eukaryotes},
