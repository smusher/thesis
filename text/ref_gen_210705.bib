	author = {Gloyn, Anna L. and Diatloff-Zito, Catherine and Edghill, Emma L. and Bellanné-Chantelot, Christine and Nivot, Sylvie and Coutant, Régis and Ellard, Sian and Hattersley, Andrew T. and Robert, Jean Jacques},
