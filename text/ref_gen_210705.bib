	keywords = {{KATP} channel, {ATP}-sensitive K+ channel, {SUR}, sulfonylurea receptor, [{ATP}]i, intracellular {ATP} concentration, {hetR}50P, heterozygous R50P, {hetR}50Q, heterozygous R50Q, {homR}50P, homomeric R50P, {homR}50Q, homomeric R50Q, {IC}50, half-maximal inhibitory concentration, {PNDM}, permanent neonatal diabetes},
	file = {Full Text PDF:/home/sam/Zotero/storage/JBFARLN5/Shimomura et al. - 2006 - Mutations at the Same Residue (R50) of Kir6.2 (KCN.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/2UDCR2CV/1705.html:text/html},
