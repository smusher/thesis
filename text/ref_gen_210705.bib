	file = {Full Text PDF:/home/sam/Zotero/storage/IKMF9DAV/Kim and Nimigean - 2016 - Voltage-Gated Potassium Channels A Structural Exa.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/XFDSEPML/a029231.html:text/html},
}

@article{cuello_structural_2010,
