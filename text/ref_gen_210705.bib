	note = {Number: 5
Publisher: Nature Publishing Group},
	file = {Full Text PDF:/home/sam/Zotero/storage/32KENK7Y/Haider et al. - 2019 - Prognostic prediction models for diabetic retinopa.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/EZXWP6MQ/s41433-018-0322-x.html:text/html},
