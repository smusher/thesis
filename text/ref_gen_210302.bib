@article{proks_heterozygous_2006,
	title = {A heterozygous activating mutation in the sulphonylurea receptor {SUR1} ({ABCC8}) causes neonatal diabetes},
	volume = {15},
	issn = {0964-6906},
	url = {https://doi.org/10.1093/hmg/ddl101},
	doi = {10.1093/hmg/ddl101},
	abstract = {Neonatal diabetes is a genetically heterogeneous disorder with nine different genetic aetiologies reported to date. Heterozygous activating mutations in the KCNJ11 gene encoding Kir6.2, the pore-forming subunit of the ATP-sensitive potassium (KATP) channel, are the most common cause of permanent neonatal diabetes. The sulphonylurea receptor (SUR) SUR1 serves as the regulatory subunit of the KATP channel in pancreatic beta cells. We therefore hypothesized that activating mutations in the ABCC8 gene, which encodes SUR1, might cause neonatal diabetes. We identified a novel heterozygous mutation, F132L, in the ABCC8 gene of a patient with severe developmental delay, epilepsy and neonatal diabetes (DEND syndrome). This mutation had arisen de novo and was not present in 150 control chromosomes. Residue F132 shows evolutionary conservation across species and is located in the first set of transmembrane helices (TMD0) of SUR1, which is proposed to interact with Kir6.2. Functional studies of recombinant KATP channels demonstrated that F132L markedly reduces the sensitivity of the KATP channel to inhibition by MgATP and this increases the whole-cell KATP current. The functional consequence of this ABCC8 mutation mirrors that of KCNJ11 mutations causing neonatal diabetes and provides new insights into the interaction of Kir6.2 and SUR1. As SUR1 is expressed in neurones as well as in beta cells, this mutation can account for both neonatal diabetes and the neurological phenotype. Our results demonstrate that SUR1 mutations constitute a new genetic aetiology for neonatal diabetes and that they act by reducing the KATP channel's ATP sensitivity.},
	number = {11},
	urldate = {2021-03-14},
	journal = {Human Molecular Genetics},
	author = {Proks, Peter and Arnold, Amanda L. and Bruining, Jan and Girard, Christophe and Flanagan, Sarah E. and Larkin, Brian and Colclough, Kevin and Hattersley, Andrew T. and Ashcroft, Frances M. and Ellard, Sian},
	month = jun,
	year = {2006},
	pages = {1793--1800},
	file = {Full Text PDF:/home/sam/Zotero/storage/HQTMTZAP/Proks et al. - 2006 - A heterozygous activating mutation in the sulphony.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/GYH6U3CB/592197.html:text/html},
}

@article{cosgrove_genetics_2004,
	title = {Genetics and {Pathophysiology} of {Hyperinsulinism} in {Infancy}},
	volume = {61},
	issn = {1663-2818, 1663-2826},
	url = {https://www.karger.com/Article/FullText/76933},
	doi = {10.1159/000076933},
	abstract = {Hyperinsulinism in infancy (HI) is a condition of neonates and early childhood. For many years the pathophysiology of this potentially lethal disorder was unknown. Advances in the genetics, histopathology and molecular physiology of this disease have now provided insights into the causes of beta-cell dysfunction and revealed levels of diversity far in excess of our previous knowledge. These include defects in ion channel subunit genes and mutations in several enzymes associated with beta-cell metabolism and anaplerosis. In most cases, beta-cell pathophysiology leads to an alteration in the function of ATP-sensitive K$^{\textrm{+}}$ channels. This can manifest as ‘channelopathies’ of K$_{\textrm{ATP}}$ channels through gene defects in \textit{ABCC8} and \textit{KCNJ11} (Ch.11p15); or as a result of ‘metabolopathies’ through defects in the genes encoding glucokinase (\textit{GCK}, Ch.7p15–p13), glutamate dehydrogenase (\textit{GLUD1}, Ch.10q23.3) and short-chain L-3-hydroxyacyl-CoA dehydrogenase (\textit{HADHSC}, Ch.4q22–q26). This review focuses upon the relationship between the causes of HI and therapeutic options.},
	language = {english},
	number = {6},
	urldate = {2021-03-14},
	journal = {Hormone Research in Paediatrics},
	author = {Cosgrove, Karen E. and Shepherd, Ruth M. and Fernandez, Eva M. and Natarajan, Anuja and Lindley, Keith J. and Aynsley-Green, Albert and Dunne, Mark J.},
	year = {2004},
	pmid = {14981344},
	note = {Publisher: Karger Publishers},
	pages = {270--288},
	file = {Full Text PDF:/home/sam/Zotero/storage/7RN3628N/Cosgrove et al. - 2004 - Genetics and Pathophysiology of Hyperinsulinism in.pdf:application/pdf},
}

