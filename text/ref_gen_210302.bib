	note = {Publisher: American Diabetes Association
Section: Original Research},
	pages = {3909--3919},
	file = {Full Text PDF:/home/sam/Zotero/storage/GTS277IX/Proks et al. - 2013 - Molecular Mechanism of Sulphonylurea Block of KATP.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/76XFH7ST/3909.html:text/html},
