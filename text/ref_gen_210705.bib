	file = {ScienceDirect Full Text PDF:/home/sam/Zotero/storage/BUKDVUN3/Pratt et al. - 2009 - Sulfonylurea Receptor 1 Mutations That Cause Oppos.pdf:application/pdf;ScienceDirect Snapshot:/home/sam/Zotero/storage/96PUJR4X/S0021925820325072.html:text/html},
}

