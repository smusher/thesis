	number = {12},
	journaltitle = {Journal of Biological Chemistry},
	shortjournal = {Journal of Biological Chemistry},
	author = {Pratt, Emily B. and Yan, Fei-Fei and Gay, Joel W. and Stanley, Charles A. and Shyng, Show-Ling},
	urldate = {2021-03-19},
	date = {2009-03-20},
