	pages = {2551--2557},
	number = {7},
	journaltitle = {The Journal of Clinical Endocrinology \& Metabolism},
	shortjournal = {The Journal of Clinical Endocrinology \& Metabolism},
	author = {Craig, Tim J. and Shimomura, Kenju and Holl, Reinhard W. and Flanagan, Sarah E. and Ellard, Sian and Ashcroft, Frances M.},
	urldate = {2021-03-18},
	date = {2009-07-01},
	file = {Full Text PDF:/home/sam/Zotero/storage/GQI92Z2U/Craig et al. - 2009 - An In-Frame Deletion in Kir6.2 (KCNJ11) Causing Ne.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/NRA4R9NJ/2597232.html:text/html},
}

