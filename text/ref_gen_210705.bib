@article{matsuo_katp_2005,
	title = {{KATP} channel interaction with adenine nucleotides},
	volume = {38},
	issn = {0022-2828},
	url = {https://www.sciencedirect.com/science/article/pii/S002228280400361X},
	doi = {10.1016/j.yjmcc.2004.11.021},
	abstract = {{ATP}-sensitive potassium ({KATP}) channels are regulated by adenine nucleotides to convert changes in cellular metabolic levels into membrane excitability. Hence, elucidation of interaction of {SUR} and Kir6.x with adenine nucleotides is an important issue to understand the molecular mechanisms underlying the metabolic regulation of the {KATP} channels. We analyzed direct interactions with adenine nucleotides of each subunit of {KATP} channels. Kir6.2 binds adenine nucleotides in a Mg2+-independent manner. {SUR} has two {NBFs} which are not equivalent: {NBF}1 is a Mg2+-independent high affinity nucleotide binding site, whereas {NBF}2 is a Mg-dependent low affinity site. Although {SUR} has {ATPase} activity at {NBF}2, it is not used to transport substrates against the concentration gradient unlike other {ABC} proteins. The {ATPase} cycle at {NBF}2 serves as a sensor of cellular metabolism. This may explain the low {ATP} hydrolysis rate compared to other {ABC} proteins. Based on studies of photoaffinity labeling, a model of {KATP} channel regulation is proposed, in which {KATP} channel activity is regulated by {SUR} via monitoring the intracellular {MgADP} concentration. {KATP} channel activation is expected to be induced by the cooperative interaction of {ATP} binding at {NBF}1 and {MgADP} binding at {NBF}2.},
	pages = {907--916},
	number = {6},
	journaltitle = {Journal of Molecular and Cellular Cardiology},
	shortjournal = {Journal of Molecular and Cellular Cardiology},
	author = {Matsuo, Michinori and Kimura, Yasuhisa and Ueda, Kazumitsu},
	urldate = {2021-03-09},
	date = {2005-06-01},
	langid = {english},
	keywords = {{ABC} proteins, Adenine nucleotides {ATP}, K channels, Photoaffinity labeling},
	file = {ScienceDirect Snapshot:/home/sam/Zotero/storage/DIR9QUFU/S002228280400361X.html:text/html;Matsuo et al. - 2005 - KATP channel interaction with adenine nucleotides.pdf:/home/sam/Zotero/storage/C7DUDBSX/Matsuo et al. - 2005 - KATP channel interaction with adenine nucleotides.pdf:application/pdf},
}

@article{ueda_mgadp_1997,
	title = {{MgADP} Antagonism to Mg2+-independent {ATP} Binding of the Sulfonylurea Receptor {SUR}1 *},
	volume = {272},
	issn = {0021-9258, 1083-351X},
	url = {https://www.jbc.org/article/S0021-9258(19)65531-6/abstract},
	doi = {10.1074/jbc.272.37.22983},
	abstract = {{\textless}p{\textgreater}Pancreatic beta-cell {ATP}-sensitive potassium (K$_{\textrm{{ATP}}}$) channels play an important role in the regulation of glucose-induced insulin secretion. The beta-cell K$_{\textrm{{ATP}}}$ channel comprises two subunits, the sulfonylurea receptor {SUR}1, a member of the {ATP}-binding cassette ({ABC}) superfamily, and Kir6.2, a member of the inward rectifier K$^{\textrm{+}}$ channel family. The activity of the K$_{\textrm{{ATP}}}$ channel is under complex regulation by the intracellular {ATP} and {ADP}. To understand the roles of the two nucleotide-binding folds ({NBFs}) of {SUR}1 in the regulation of K$_{\textrm{{ATP}}}$ channel activity, we introduced point mutations into the core consensus sequence of the Walker A or B motif of each {NBF} of {SUR}1 and characterized {ATP} binding and {ADP} or {MgADP} antagonism to it. {SUR}1 was efficiently photolabeled with 8-azido-[α-$^{\textrm{32}}$P]{ATP} and 8-azido-[γ-$^{\textrm{32}}$P]{ATP} in the presence or absence of Mg$^{\textrm{2+}}$ or vanadate. {NBF}1 mutations impaired {ATP} binding, but {NBF}2 mutations did not. {MgADP} strongly antagonized {ATP} binding, and the {NBF}2 mutation reduced {MgADP} antagonism. These results show that {SUR}1, unlike other {ABC} proteins, strongly binds {ATP} at {NBF}1 even in the absence of Mg$^{\textrm{2+}}$ and that {MgADP}, through binding at {NBF}2, antagonizes the Mg$^{\textrm{2+}}$-independent high affinity {ATP} binding at {NBF}1.{\textless}/p{\textgreater}},
	pages = {22983--22986},
	number = {37},
	journaltitle = {Journal of Biological Chemistry},
	shortjournal = {Journal of Biological Chemistry},
	author = {Ueda, Kazumitsu and Inagaki, Nobuya and Seino, Susumu},
	urldate = {2021-03-09},
	date = {1997-09-12},
	file = {Full Text PDF:/home/sam/Zotero/storage/YBZ25EI4/Ueda et al. - 1997 - MgADP Antagonism to Mg2+-independent ATP Binding o.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/ZHKTMKCA/fulltext.html:text/html},
}

@article{martin_pharmacological_2016,
