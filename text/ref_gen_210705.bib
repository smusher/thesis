Publisher: Nature Publishing Group},
	file = {Full Text PDF:/home/sam/Zotero/storage/PAWQ53UP/Ashcroft et al. - 1984 - Glucose induces closure of single potassium channe.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/P75XPSFI/312446a0.html:text/html},
}

