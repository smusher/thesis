	year = {2009},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/humu.20838},
	keywords = {sulfonylurea receptor, Kir6.2, potassium channel, SUR1, ABCC8, hyperinsulinemia of infancy, inwardly rectifying, KCNJ11, permanent neonatal diabetes, transient neonatal diabetes},
	pages = {170--180},
	file = {Full Text PDF:/home/sam/Zotero/storage/PVQJKGEA/Flanagan et al. - 2009 - Update of mutations in the genes encoding the panc.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/2QYZ2LAH/humu.html:text/html},
