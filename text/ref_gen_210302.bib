	pages = {101--110.e10},
	file = {ScienceDirect Full Text PDF:/home/sam/Zotero/storage/ASNH83G7/Li et al. - 2017 - Structure of a Pancreatic ATP-Sensitive Potassium .pdf:application/pdf;ScienceDirect Snapshot:/home/sam/Zotero/storage/2PIJ2FWQ/S0092867416317445.html:text/html},
}

@article{inagaki_subunit_1997,
	title = {Subunit stoichiometry of the pancreatic beta-cell {ATP}-sensitive {K}+ channel},
	volume = {409},
	copyright = {FEBS Letters 409 (1997) 1873-3468 © 2015 Federation of European Biochemical Societies},
	issn = {1873-3468},
	url = {https://febs.onlinelibrary.wiley.com/doi/abs/10.1016/S0014-5793%2897%2900488-2},
	doi = {https://doi.org/10.1016/S0014-5793(97)00488-2},
	abstract = {We have investigated the subunit stoichiometry of the pancreatic beta-cell ATP-sensitive K+ (KATP) channel (SUR1/Kir6.2 channel) by constructing cDNA encoding a single polypeptide (betaα polypeptide) consisting of a SUR1 (beta) subunit and a Kir6.2 (α) subunit. 86Rb+ efflux and single-channel properties of COS1 cells expressing betaα polypeptides were similar to those of COS1 cells coexpressing α monomers and beta monomers. Coexpression of betaα polypeptides with α monomers inhibited the K+ currents, while coexpression with beta monomers did not. We then constructed another single polypeptide (betaα2) consisting of a beta subunit and a dimeric repeat of the α subunit. 86Rb+ efflux from COS1 cells expressing betaα2 polypeptides was small, but was restored by supplementation with beta monomers. These results indicate that the activity of KATP channels is optimized when the α and beta subunits are coexpressed with a molar ratio of 1:1. Since inward rectifier K+ channels are thought to function as homo- or hetero-tetramers, this suggests that the KATP channel functions as a multimeric protein, most likely a hetero-octamer composed of a tetramer of the Kir6.2 subunit and a tetramer of the SUR1 subunit.},
	language = {en},
