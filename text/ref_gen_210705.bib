@article{tammaro_mutation_2007,
	title = {A mutation in the {ATP}-binding site of the Kir6.2 subunit of the {KATP} channel alters coupling with the {SUR}2A subunit},
	volume = {584},
	rights = {© 2007 The Journal of Physiology © 2007 The Physiological Society},
