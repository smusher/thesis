	pages = {337--350},
	file = {Full Text PDF:/home/sam/Zotero/storage/QNMWUB46/Sakura et al. - 1999 - Altered functional properties of KATP channel conf.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/BIBNDGR3/j.1469-7793.1999.00337.html:text/html},
