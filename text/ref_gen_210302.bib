	pages = {337--350},
	file = {Snapshot:/home/sam/Zotero/storage/7WJN38KV/j.1469-7793.1999.00337.html:text/html;Full Text PDF:/home/sam/Zotero/storage/GLCH3TKE/Sakura et al. - 1999 - Altered functional properties of KATP channel conf.pdf:application/pdf},
}

