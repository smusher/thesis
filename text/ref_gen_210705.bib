	langid = {english},
	note = {\_eprint: https://febs.onlinelibrary.wiley.com/doi/pdf/10.1016/S0014-5793\%2897\%2900488-2},
	keywords = {{ATP}, Inward rectifier, Potassium channel, Stoichiometry, Sulfonylurea receptor},
	file = {Full Text PDF:/home/sam/Zotero/storage/AS7RHH8Z/Inagaki et al. - 1997 - Subunit stoichiometry of the pancreatic beta-cell ATP.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/SVGDKTUM/S0014-5793(97)00488-2.html:text/html},
