	langid = {english},
	file = {Full Text PDF:/home/sam/Zotero/storage/83FJ4KM9/Grosman et al. - 2000 - Mapping the conformational wave of acetylcholine r.pdf:application/pdf;Snapshot:/home/sam/Zotero/storage/UMHH3KTB/35001586.html:text/html},
}

